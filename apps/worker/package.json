{
  "name": "garage-comics-worker",
  "version": "1.0.0",
  "description": "Worker para procesar pedidos desde RabbitMQ",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "dotenv -- tsx watch src/index.ts",
    "build": "tsc",
    "test:template": "dotenv -- tsx test-template.ts",
    "test:worker": "dotenv -- tsx test-worker-simulation.ts",
    "test:flow": "dotenv -- tsx test-complete-flow.ts",
    "test:simple": "dotenv -- tsx test-flow-simple.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@react-email/components": "^0.5.1",
    "@react-email/render": "^1.2.1",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.2",
    "pg-boss": "^10.3.2",
    "react": "^19.1.1",
    "resend": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^19.1.11",
    "dotenv-cli": "^10.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
